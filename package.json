{
  "name": "@alwaystudios/as-react-hooks",
  "version": "1.0.2",
  "repository": "git@github.com:alwaystudios/as-react-hooks.git",
  "author": "Gary Alway <garyalway@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "test": "jest --maxWorkers=4 src --coverage",
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write 'src/**/*.{js,ts,tsx,json,css,md}'",
    "build": "rollup -c",
    "clean": "sh cleanup.sh",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn format && yarn test",
      "pre-push": "yarn build && yarn test"
    }
  },
  "dependencies": {
    "@alwaystudios/as-utils": "^1.0.16",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "universal-cookie": "^4.0.4"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/jest": "^26.0.23",
    "@types/ramda": "^0.27.40",
    "@types/react": "^17.0.8",
    "@types/universal-cookie": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-plugin-deprecation": "^1.2.1",
    "eslint-plugin-functional": "^3.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-react": "^7.24.0",
    "husky": "4.3.0",
    "jest": "^27.0.3",
    "prettier": "^2.3.0",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.50.4",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.1",
    "typescript": "^4.3.2"
  }
}
